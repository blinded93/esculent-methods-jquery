{{#users}}
  <div id="user-{{id}}" class="imageBox result">
    <img class="rounded" src="{{thumbURL}}">
    <div class="ml-3 small">
      <a href="" class="userLink black h6">{{username}}</a>
      {{#ifLoggedIn}}
        {{#ifFriend id}}
          <a href="" class="removeFriend">
            <img src="/assets/icons/remove-friend-bw.png" alt="Remove Friend" class="friendIcon removeFriendImg" title="Remove Friend">
          </a>
          {{#ifPending id}}
            <small class="text-success">Pending</small>
          {{/ifPending}}
        {{else}}
        <a href="" class="addFriend">
          <img src="/assets/icons/add-friend-bw.png" alt="Add Friend" class="friendIcon addFriendImg" title="Add Friend">
        </a>
        {{/ifFriend}}
      {{/ifLoggedIn}}
      <br>
      <a href="" class="favoritesLink">Favorites</a> | <a href="" class="recipesLink">Recipes</a>
    </div>
  </div>
{{/users}}
<div id="paginationNav">
</div>
